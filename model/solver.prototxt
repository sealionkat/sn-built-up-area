net: "model/train.prototxt"

# Liczba iteracji testowania
test_iter: 5

# Testowanie co X iteracji uczenia
test_interval: 20

# Współczynniki
base_lr: 0.01
momentum: 0.9
weight_decay: 0.0005


lr_policy: "inv"
gamma: 0.0001
power: 0.75

# Wypisywanie co X iteracji
display: 5

# Max liczba iteracji (37x37 => 1369 / 25 => 55 * 448 => 25k)
# 55 * 10 * 8 = 5k
max_iter: 100000


snapshot: 1000
snapshot_prefix: "snapshots/"

solver_mode: CPU
