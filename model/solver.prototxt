net: "model/train.prototxt"

# Liczba iteracji testowania
test_iter: 5

# Testowanie co X iteracji uczenia
test_interval: 20

# Współczynniki
base_lr: 0.01
momentum: 0.9
weight_decay: 0.0005


lr_policy: "inv"
gamma: 0.0001
power: 0.75

# Wypisywanie co X iteracji
display: 1

# Max liczba iteracji (21*21 => 441 / 49 => 9 * 448 => 4032)
max_iter: 4032


snapshot: 5000
snapshot_prefix: "snapshots/"

solver_mode: CPU